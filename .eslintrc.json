{
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "require-jsdoc": "off",
        "spaced-comment": "off",
        "eqeqeq": "error"
    }
}